{
	"DbInitialize": "true",
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "Data Source=localhost;Initial Catalog=jo;User ID=LOIC;Password=loic;TrustServerCertificate=true"
	},
	"StationConfig": {
		"StationName": "S5chained",
		"InstanceName": "ATS Match4 DX"
	},
	"VisionSettings": {
		"FolderParams": "C:\\d\\ADSVision\\DLLVision\\params",
		"DLLPath": "C:\\d\\ADSVision\\DLLVision\\V3",
		"SignMatchTimer": 1,
		"InstanceMatchID": 4,
		"AnodeType": "01",
		"StationDelay": 0.02,
		"IsChained": "true",
		"GPUID": [
			0,
			0
		],
		"GoMatchStations": [
			"S3",
			"S4"
		]
	},
	"FileSettings": {
		"FileSettingTimer": 10000,
		"ArchivePath": "C:\\d\\ADSVision\\DLLVision\\Archive\\"
	},
	"CameraConfig": {
		"ImagesPath": "C:\\ats\\images\\picref\\images\\",
		"ThumbnailsPath": "C:\\ats\\images\\picref\\thumbnails\\",
		"Extension": "jpg"
	},
	"DebugConfig": {
		"DebugFolderPath": "C:\\ats\\Services\\Match\\Gate2Gate1DX\\Match4\\Debug",
		"LogFilePath": "C:\\ats\\Services\\Match\\Gate2Gate1DX\\Match4\\Log",
		"CsvExportFolderPath": "C:\\ats\\Services\\Match\\Gate2Gate1DX\\Match4\\CSV"
	},
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.MSSqlServer" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"System": "Warning",
				"Microsoft": "Warning",
				"Microsoft.AspNetCore": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
					"outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} - {Source} - {Instance}] {Message:lj}{NewLine}{Exception}"
				}
			},
			{
				"Name": "MSSqlServer",
				"Args": {
					"connectionString": "Data Source=localhost;Initial Catalog=jo;User ID=LOIC;Password=loic;TrustServerCertificate=true",
					"sinkOptionsSection": {
						"autoCreateSqlDatabase": false,
						"autoCreateSqlTable": false,
						"tableName": "Logs"
					},
					"columnOptionsSection": {
						"id": {
							"columnName": "ID"
						},
						"timeStamp": {
							"columnName": "TS"
						},
						"additionalColumns": [
							{
								"ColumnName": "Source",
								"DataType": "nvarchar",
								"DataLength": 50,
								"AllowNull": false
							},
							{
								"ColumnName": "Instance",
								"DataType": "nvarchar",
								"DataLength": 50,
								"AllowNull": true
							},
							{
								"ColumnName": "HasBeenSent",
								"DataType": "bit",
								"AllowNull": false
							}
						]
					}
				}
			}
		],
		"Enrich": [
			"FromLogContext"
		]
	}
}